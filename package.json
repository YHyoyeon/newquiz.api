{
    "name": "newquiz.api",
    "version": "0.0.0",
    "description": "New-Quiz Api Server",
    "author": "newquiz.api <gydus.dev@gmail.com>",
    "scripts": {
      "build": "rm -rf ./dist ; tsc --build",
      "start:news:dev": "INSTANCES=2 pm2 start ecosystem.config.js --env dev --only news_server",
      "news:nodemon": "nodemon --watch src --ext ts --exec ts-node src/app_news/app.ts",
      "news": "ts-node src/app_news/app.ts",
      "lint": "eslint --ext .ts ./src"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/YHyoyeon/newquiz.api.git"
    },
    "lint-staged": {
      "*.ts": [
        "eslint --cache --fix"
      ]
    },
    "private": true,
    "dependencies": {
      "compression": "^1.7.5",
      "cors": "^2.8.5",
      "express": "^4.21.1",
      "express-rate-limit": "^7.4.1",
      "extend": "^3.0.2",
      "geoip-lite": "^1.4.10",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.1",
      "uuid": "^11.0.3",
      "winston": "^3.17.0",
      "winston-daily-rotate-file": "^5.0.0",
      "xss-clean": "^0.1.4",
      "zod": "^3.23.8"
    },
    "devDependencies": {
      "@types/compression": "^1.7.5",
      "@types/cors": "^2.8.17",
      "@types/debug": "^4.1.7",
      "@types/express": "^5.0.0",
      "@types/extend": "^3.0.4",
      "@types/node": "^18.8.3",
      "@types/swagger-ui-express": "^4.1.7",
      "ts-node": "^10.9.2",
      "typescript": "^5.6.3"
    },
    "engine": "node >= 12.3.0"
  }
  